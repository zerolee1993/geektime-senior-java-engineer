{
    "title": "极客时间Java高级工程师体系课笔记",
    "description": "极客时间Java高级工程师体系课笔记",
    "author": "lishaofei",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "links" : {
        "sidebar" : {
            "Home" : "https://www.baidu.com"
        }
    },
    "plugins": [
        "copy-code-button",
        "-lunr", "-search", "search-plus", "search-pro",
        "github",
        "emphasize",
        "splitter",
        "-sharing",
        "-highlight",
        "tbfed-pagefooter",
        "anchors",
        "-livereload",
        "simple-page-toc",
        "prism",
        "prism-themes",
        "sectionx",
        "back-to-top-button",
        "cuav-chapters",
        "heading-anchors",
        "theme-fexa",
        "popup",
        "pageview-count"
    ],
    "pluginsConfig": {
        "sytles": {
            "website": "styles/website.css"
        },
        "fontsettings": {
            "theme": "white",
            "family": "sans",
            "size": 1
        },
        "github": {
            "url": "https://github.com/zerolee1993/geektime-senior-java-engineer"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-okaidia.css"
            ]
        },
        "theme-default": {
            "showLevel": false
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © zerolee 2022",
            "modify_label": "修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "sectionx": {
            "tag": "b"
        },
        "theme-fexa":{
            "search-placeholder":"你想搜啥？",
            "logo":"assets/images/logo.png",
            "favicon": "assets/images/favicon.ico"
        },
        "insert-logo": {
            "url": "assets/images/logo.png",
            "sytle": "background: none; max-hight: 30px; min-height: 30px"
        }
    }
}
